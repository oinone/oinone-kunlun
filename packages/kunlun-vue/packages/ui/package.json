    "vue3-smooth-dnd": "0.0.5",
    "vuedraggable": "4.1.0",
    "vxe-table": "4.3.6",
    "vxe-table-plugin-antd": "3.0.6",
    "vxe-table-plugin-element": "3.0.6",
    "xe-utils": "3.5.7"
  },
  "devDependencies": {
    "@oinone/kunlun-shared": "~6.2.0",
    "@types/lodash": "4.14.182",
    "@types/lodash-es": "4.17.6",
    "lodash": "4.17.21",
    "lodash-es": "4.17.21",
    "sass": "1.44.0",
    "sass-loader": "10.1.1"
  },
  "peerDependencies": {
    "vue": ">=3.2.40",
