{
  "name": "@kunlun/vue-ui-common",
  "version": "6.2.0",
  "main": "index.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c && npm run build-css",
    "build-css": "sass --style compressed --no-source-map ./dist/kunlun-vue-ui-common.scss ./dist/kunlun-vue-ui-common.css",
    "dist": "npm run prepublishOnly",
    "index": "npm run postpublish",
    "x-prepublishOnly": "node ../../scripts/prepublish-only.js",
    "x-postpublish": "node ../../scripts/postpublish.js",
    "doc": "typedoc --out docs src/index.ts"
  },
  "dependencies": {
    "lodash": "4.17.21",
    "lodash-es": "4.17.21"
  },
  "devDependencies": {
    "@kunlun/engine": "~6.2.0",
    "@kunlun/meta": "~6.2.0",
    "@kunlun/request": "~6.2.0",
    "@kunlun/shared": "~6.2.0",
    "@types/lodash": "4.14.182",
    "@types/lodash-es": "4.17.6",
    "rollup-plugin-copy": "3.4.0",
    "sass": "1.44.0",
    "sass-loader": "10.1.1"
  },
  "peerDependencies": {
    "vue": ">=3.2.40"
  },
  "gitHead": "2b4c0c7911626c105ca71c2d41e54af470e2e079"
}
