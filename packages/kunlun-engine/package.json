{
  "name": "@kunlun/engine",
  "version": "6.2.0",
  "main": "index.ts",
  "description": "kunlun-engine",
  "scripts": {
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "clean": "rimraf dist package-lock.json node_modules",
    "lint": "lint-staged && node ./scripts/check-keyword.js",
    "lint:all": "eslint --quiet --fix",
    "check-keyword": "bash ./scripts/check-keyword.sh",
    "patch": "patch-package",
    "test": "jest",
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "build-css": "sass --style compressed --no-source-map ./dist/kunlun-engine.scss ./dist/kunlun-engine.css",
    "dist": "npm run prepublishOnly",
    "index": "npm run postpublish",
    "x-prepublishOnly": "node ./scripts/prepublish-only.js",
    "x-postpublish": "node ./scripts/postpublish.js",
    "doc": "typedoc --out docs src/index.ts",
    "deploy": "npm publish --exact",
    "format": "prettier --write --parser typescript \"packages/**/*.ts\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@kunlun/cache": "~6.2.0",
    "@kunlun/dsl": "~6.2.0",
    "@kunlun/environment": "~6.2.0",
    "@kunlun/event": "~6.2.0",
    "@kunlun/expression": "~6.2.0",
    "@kunlun/meta": "~6.2.0",
    "@kunlun/request": "~6.2.0",
    "@kunlun/router": "~6.2.0",
    "@kunlun/service": "~6.2.0",
    "@kunlun/shared": "~6.2.0",
    "@kunlun/spi": "~6.2.0",
    "@kunlun/state": "~6.2.0",
    "@kunlun/theme": "~6.2.0",
    "bignumber.js": "9.0.0",
    "lodash": "4.17.21",
    "lodash-es": "4.17.21"
  },
  "devDependencies": {
    "@commitlint/cli": "9.1.2",
    "@commitlint/config-conventional": "9.1.2",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "11.2.1",
    "@rollup/plugin-replace": "2.4.2",
    "@types/jest": "~29.5.14",
    "@types/lodash": "4.14.182",
    "@types/lodash-es": "4.17.6",
    "@typescript-eslint/parser": "5.5.0",
    "eslint": "7.9.0",
    "husky": "4.2.5",
    "jest": "~29.7.0",
    "jest-environment-jsdom": "~29.7.0",
    "lint-staged": "10.4.2",
    "patch-package": "6.2.2",
    "prettier": "2.5.0",
    "rimraf": "3.0.2",
    "rollup": "2.63.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-scss": "4.0.0",
    "rollup-plugin-sourcemaps": "0.6.3",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.29.0",
    "rollup-plugin-visualizer": "^5.5.4",
    "ts-jest": "29.3.0",
    "tslib": "2.3.1",
    "typedoc": "0.20.37",
    "typescript": "4.5.2"
  },
  "gitHead": "2b4c0c7911626c105ca71c2d41e54af470e2e079"
}